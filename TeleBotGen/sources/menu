#!/bin/bash



P_SERVER="https://www.dropbox.com/s/blxo0jifysvyrey/"
v1=$(curl -sSL "${P_SERVER}/v-new.log")
v2=$(cat $SCPT_DIR/v-local.log)
txt[315]="üí• ChumoGH Keygen üí• IS UPDATED!"
txt[316]="‚ö†Ô∏è ChumoGH Keygen ‚ö†Ô∏è NEEDS UPDATE!"
[[ $v1 = $v2 ]] && vesaoSCT="\033[1;32m${txt[315]} \033[0;33m ($v2)" || vesaoSCT="\033[1;31m${txt[316]}\033[0;33m($v2) ‚ñ∫ \033[1;32m($v1)\033[1;31m"

menu_src () {
bot_retorno="‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢ M E N U ‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢\n"
	 if [[ $(echo $permited|grep "${chatuser}") = "" ]]; then
		 if [[ $(cat ${CID}|grep "${chatuser}") = "" ]]; then
			 bot_retorno+="/ID (muestra sus ID)\n"
			 bot_retorno+="/menu (muestra este menu)\n"
			 bot_retorno+="/ayuda (ayuda de los comandos)\n"
			 bot_retorno+="$LINE\n"
			 msj_fun
		 else
			 bot_retorno+="/ID (muestra sus ID)\n"
			 bot_retorno+="/instal (link de instalacion)\n"
			 bot_retorno+="/menu (muestra este menu)\n"
			 bot_retorno+="/ayuda (ayuda de los comandos)\n"
			 bot_retorno+="$LINE\n"
			 menu_print
		 fi
		 
	 else
		 unset PID_GEN
		 PID_GEN=$(ps x|grep -v grep|grep "http-server.sh")
		 [[ ! $PID_GEN ]] && PID_GEN='(Offline) ‚ùå' || PID_GEN='(Online) ‚úÖ'

		 unset usadas
		 usadas="$(cat /etc/http-instas)"
		 [[ ! $usadas ]] && k_used="0" || k_used="$usadas"
		 bot_retorno+="${vesaoSCT}"
		 bot_retorno+="Gen $PID_GEN | Keys Used [$k_used]\n"

		 bot_retorno+="$LINE\n"
		 bot_retorno+="/infosys (info del sistema)\n"
		 bot_retorno+="/list (lista de ID permitidas)\n"
		 bot_retorno+="/instal (link de instalacion)\n"
		 bot_retorno+="/menu (muestra este menu)\n"
		 bot_retorno+="/ayuda (ayuda de los comandos)\n"
		 bot_retorno+="/cache (Limpiar cache Ram)\n"
		 bot_retorno+="/update (actualiza el BotGen)\n"
		 bot_retorno+="/reboot (Reinicia el servidor vps)\n"
		 bot_retorno+="$LINE\n"
		 menu_print
	 fi	
}
