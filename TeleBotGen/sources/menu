#!/bin/bash



P_SERVER="https://www.dropbox.com/s/blxo0jifysvyrey/"
v1=$(curl -sSL "${P_SERVER}/v-new.log")
v2=$(cat $SCPT_DIR/v-local.log)
txt[315]="üí• ChumoGH Keygen üí• IS UPDATED!"
txt[316]="‚ö†Ô∏è ChumoGH Keygen ‚ö†Ô∏è NEEDS UPDATE!"
[[ $v1 = $v2 ]] && vesaoSCT="${txt[315]} ($v2)" || vesaoSCT="${txt[316]}($v2) ‚ñ∫ ($v1)"

menu_src () {
bot_retorno="‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢ M E N U ‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢‚Ä¢\n"
	 if [[ $(echo $permited|grep "${chatuser}") = "" ]]; then
		 if [[ $(cat ${CID}|grep "${chatuser}") = "" ]]; then
			 bot_retorno+="/ID | Muestra sus ID |\n"
			 bot_retorno+="/menu | MENU DE COMANDOS |\n"
			 bot_retorno+="/ayuda | INSTRUCCIONES DE COMANDOS |\n"
			 bot_retorno+="$LINE\n"
			 msj_fun
		 else
			 bot_retorno+="/id | Muestra sus ID |\n"
			 bot_retorno+="/instal | INSTALADOR OFICIAL |\n"
			 bot_retorno+="/menu | MENU DE COMANDOS | \n"
			 bot_retorno+="/ayuda | INSTRUCCIONES DE COMANDOS |\n"
			 bot_retorno+="$LINE\n"
			 menu_print
		 fi
		 
	 else
		 unset PID_GEN
		 
		 PID_GEN=$(ps x|grep -v grep|grep "http-server.sh")
		 [[ ! $PID_GEN ]] && PID_GEN='(Offline) ‚ùå' || PID_GEN='(Online) ‚úÖ'

		 unset usadas
		 usadas="$(cat /etc/http-instas)"
		 [[ ! $usadas ]] && k_used="0" || k_used="$usadas"
		 bot_retorno+="   ${vesaoSCT}"
		 bot_retorno+="Gen $PID_GEN | Keys Used [$k_used]\n"

		 bot_retorno+="$LINE\n"
		 bot_retorno+="/infosys | INFORMACION DE SERVIDOR |\n"
		 bot_retorno+="/list | MUESTRA LISTA DE ID PERMITIDOS\n"
		 bot_retorno+="/instal | INSTALADOR OFICIAL |\n"
		 bot_retorno+="/menu | MENU DE ACCIONES |\n"
		 bot_retorno+="/ayuda | INSTRUCCIONES DE COMANDOS |\n"
		 bot_retorno+="/cache | OPTIMIZA SERVIDOR |\n"
		 bot_retorno+="/update | ACTUALIZA BOT |\n"
		 bot_retorno+="/reboot | REINICIA SERVIDOR VPS |\n"
		 bot_retorno+="$LINE\n"
		 menu_print
	 fi	
}
